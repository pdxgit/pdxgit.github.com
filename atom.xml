---
layout: null
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

 <title>Grumpy Gits Singapore</title>
 <link href="http://grumpygits.github.io/atom.xml" rel="self"/>
 <link href="http://grumpygits.github.io/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://grumpygits.github.io</id>
 <author>
   <name>Grumpy Gits Singapore</name>
 </author>

 {% for post in site.posts %}
    {% if post.status != 'unpublished' %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://grumpygits.github.io{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://grumpygits.github.io{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
    {% endif %}
 {% endfor %}

</feed>
